  "glim_ros": {
    "config_path": "/config",
    // Viewer
    "enable_viewer": true,
    "enable_rviz": false,
    // Extension modules
    "enable_ext_dbow": false,
    "enable_ext_scancontext": false,
    "enable_ext_orbslam": false
